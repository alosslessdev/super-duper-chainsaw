{
  "expo": {
    "name": "mytodo-expo",
    "slug": "mytodo-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mytodoexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.growin.growin",
      "config": {
        "googleSignIn": {
          "apiKey": "YOUR_WEB_CLIENT_ID_FROM_GOOGLE_CLOUD_CONSOLE",
          "androidClientId": "1015456684061-86pj6f933utkjpui9uj62fmqntt5duf4.apps.googleusercontent.com"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "googleSignIn": {
          "clientId": "YOUR_WEB_CLIENT_ID.apps.googleusercontent.com"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e255604-a090-40c0-a636-5f2bf831ab85"
      },
      "googleWebClientId": "1015456684061-86pj6f933utkjpui9uj62fmqntt5duf4.apps.googleusercontent.com"
    }
  }
}
